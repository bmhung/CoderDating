<div class="col-xs-12 col-md-4">
  <div class="card card-<%= cycle('male', 'female') %>">
    <% if user.image_url %>
    <%= image_tag user.image_url %>
    <% end %>
    <div class="card-block">
      <h4 class="card-title"><%= user.name %></h4>
      <a href="#" class="btn btn-primary">Button</a>
    </div>
  </div>
</div>
